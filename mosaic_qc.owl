        <FR rdf:resource="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#Distribution"/>
        <appartiene_alla_Software_Architecture_Area rdf:resource="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#&apos;Wireless_Sensor_Network&apos;"/>
        <faultToleranceRate rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</faultToleranceRate>
        <stabilityRate rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stabilityRate>
        <rdfs:comment xml:lang="it">Memorizza i dati in memoria dei nodi del sensore e li carica da esso.</rdfs:comment>
        <rdfs:label xml:lang="en">In-network Data Repository Pattern</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#&apos;In-network_Result_Sharing&apos; -->

