    <!-- http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#Continuosly_changing_workload -->

    <owl:NamedIndividual rdf:about="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#Continuosly_changing_workload">
        <rdf:type rdf:resource="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#Software_design_pattern"/>
        <isInFamily rdf:resource="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#Application_workload"/>
