    <!-- http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#Key_value_storage -->

    <owl:NamedIndividual rdf:about="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#Key_value_storage">
        <rdf:type rdf:resource="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#Software_design_pattern"/>
        <isInFamily rdf:resource="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#Cloud_storage_offerings"/>
