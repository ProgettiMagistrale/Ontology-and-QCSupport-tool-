        <largescaleeventmanagementRate rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</largescaleeventmanagementRate>
        <lightweightmemoryRate rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</lightweightmemoryRate>
        <networkinteroperabilityRate rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</networkinteroperabilityRate>
        <popularityRate rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</popularityRate>
