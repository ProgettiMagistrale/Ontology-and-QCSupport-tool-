    </owl:NamedIndividual>
    


    <!-- http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#Predefined_resource_composition -->

    <owl:NamedIndividual rdf:about="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#Predefined_resource_composition">
        <rdf:type rdf:resource="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#Functional_requirement"/>
        <rdfs:comment xml:lang="en">Frequent resource conflicts occur in IoT applications that share resources. Conflict resolution will be required to resolve conflicts in resource allocation among multiple concurrent services or applications.</rdfs:comment>
        <rdfs:label xml:lang="en">Predefined resource composition</rdfs:label>
        <rdfs:label xml:lang="it">Predefined resource composition</rdfs:label>
    </owl:NamedIndividual>
    


