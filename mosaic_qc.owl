    <!-- http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#&apos;IoT_Gateway_Event_Subscription_Pattern&apos; -->

    <owl:NamedIndividual rdf:about="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#&apos;IoT_Gateway_Event_Subscription_Pattern&apos;">
