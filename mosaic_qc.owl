    <owl:NamedIndividual rdf:about="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#Managed_configuration">
        <rdf:type rdf:resource="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#Software_design_pattern"/>
        <isInFamily rdf:resource="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#Cloud_application_management_components"/>
        <nFR rdf:resource="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#Elasticity"/>
