        <nFR rdf:resource="http://dbpedia.org/resource/Adaptability"/>
        <nFR rdf:resource="http://dbpedia.org/resource/Data_consistency"/>
        <adaptabilityRate rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</adaptabilityRate>
        <rdfs:comment xml:lang="it">I risultati di alcuni task vengono immagazzinati fuori dal network, questi risultati vengono poi caricati nel network quando devono essere usati per un task principale.</rdfs:comment>
        <rdfs:label xml:lang="en">Out-network Result Sharing Pattern</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#&apos;Publisher:_Sensor_Data_Publication_Pattern&apos; -->

