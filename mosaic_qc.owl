    </owl:NamedIndividual>
    


    <!-- http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#&apos;AS2AS_Service_Orchestration_Pattern&apos; -->

    <owl:NamedIndividual rdf:about="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#&apos;AS2AS_Service_Orchestration_Pattern&apos;">
