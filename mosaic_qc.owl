        <portabilityRate rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</portabilityRate>
        <rdfs:comment xml:lang="it">Tutti i dati generati per quanto riguarda la configurazione e lo stato delle operazioni all’interno della rete devono essere disponibili in maniera specifica, senza tener conto della fonte di generazione dei dati, ciò è possibile condividendo il contesto dei dati.</rdfs:comment>
        <rdfs:label xml:lang="en">Shared Control-Plane context Pattern</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#&apos;State_Channel_Pattern&apos; -->

    <owl:NamedIndividual rdf:about="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#&apos;State_Channel_Pattern&apos;">
        <rdf:type rdf:resource="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#Software_design_pattern"/>
        <FR rdf:resource="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#Scalability"/>
        <appartiene_alla_Software_Architecture_Area rdf:resource="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#&apos;Blockchain_Data_Management&apos;"/>
        <nFR rdf:resource="http://dbpedia.org/resource/Algorithmic_efficiency"/>
        <nFR rdf:resource="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#Performance"/>
        <nFR rdf:resource="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#Privacy"/>
        <efficiencyRate rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</efficiencyRate>
