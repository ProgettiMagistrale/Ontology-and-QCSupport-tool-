        <rdf:type rdf:resource="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#Software_design_pattern"/>
        <appartiene_alla_Software_Architecture_Area rdf:resource="http://sisinflab.poliba.it/semanticweb/ontologies/architecturalpatterns#&apos;Cloud-based_5G_Communications&apos;"/>
