        <codemigrationRate rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</codemigrationRate>
        <contextawareRate rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</contextawareRate>
        <datapreprocessingaggregationRate rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</datapreprocessingaggregationRate>
        <devicediscoveryRate rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</devicediscoveryRate>
        <distributedRate rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</distributedRate>
